{
  "name": "react-konva-graphs",
  "description": "An experimental repo which aims to create a graph package for konva which is a 2d canvas based JavaScript framework for drawings shapes",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "homepage":"http://neoito-hub.github.io/react-konva-graphs",
  "dependencies": {
    "konva": "^7.1.4",
    "nanoid": "^3.1.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-konva": "^17.0.0-0",
    "react-scripts": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && mkdir dist && babel ./src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy":"npm run build",
    "deploy":"gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neoito-hub/react-konva-graphs.git"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "gh-pages": "^3.1.0",
    "web-vitals": "^0.2.4"
  }
}
